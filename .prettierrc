{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "ignore",
  "fluid": false,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": { "parser": "html" }
    }
  ]
}